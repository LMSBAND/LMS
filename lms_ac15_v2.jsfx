    bc_my_slot >= 0 ? (
      sb = BC_MY_REGION + bc_my_slot * BC_SLOT_SIZE;
      gmem[sb + 0] = 0;
      gmem[sb + 1] = bc_my_id;
      gmem[sb + 2] = BC_MY_TYPE;
      gmem[sb + 3] = bc_following;
      gmem[sb + 4] = BC_PARAM_COUNT;
    );
  );
);

bc_my_slot >= 0 && bc_following > 0 ? (
  bc_found_leader = 0;
  bc_scan = 0;
  loop(BC_MAX_INST,
    bc_scan != bc_my_slot ? (
      lsb = BC_MY_REGION + bc_scan * BC_SLOT_SIZE;
      gmem[lsb + 1] == bc_following ? (
        bc_found_leader = 1;
        bc_follow_slot = bc_scan;
        lbase = lsb + 8;
        slider1 = gmem[lbase + 0];
        slider2 = gmem[lbase + 1];
        slider3 = gmem[lbase + 2];
        slider4 = gmem[lbase + 3];
        slider5 = gmem[lbase + 4];
        slider6 = gmem[lbase + 5];
        slider7 = gmem[lbase + 6];
        slider8 = gmem[lbase + 7];
        slider9 = gmem[lbase + 8];
        slider10 = gmem[lbase + 9];
        slider11 = gmem[lbase + 10];
        slider12 = gmem[lbase + 11];
        slider13 = gmem[lbase + 12];
        slider14 = gmem[lbase + 13];
        bc_scan = BC_MAX_INST;
      );
    );
    bc_scan += 1;
  );
  !bc_found_leader ? (
    bc_following = 0;
    bc_follow_slot = -1;
  );
);

bc_my_slot >= 0 && bc_steal_pending ? (
  bc_scan = 0;
  loop(BC_MAX_INST,
    lsb = BC_MY_REGION + bc_scan * BC_SLOT_SIZE;
    gmem[lsb + 1] == bc_steal_target ? (
      lbase = lsb + 8;
      slider1 = gmem[lbase + 0];
      slider2 = gmem[lbase + 1];
      slider3 = gmem[lbase + 2];
      slider4 = gmem[lbase + 3];
      slider5 = gmem[lbase + 4];
      slider6 = gmem[lbase + 5];
      slider7 = gmem[lbase + 6];
      slider8 = gmem[lbase + 7];
      slider9 = gmem[lbase + 8];
      slider10 = gmem[lbase + 9];
      slider11 = gmem[lbase + 10];
      slider12 = gmem[lbase + 11];
      slider13 = gmem[lbase + 12];
      slider14 = gmem[lbase + 13];
      bc_scan = BC_MAX_INST;
    );
    bc_scan += 1;
  );
  bc_steal_pending = 0;
  bc_steal_target = 0;
  bc_steal_mode = 0;
);

bc_my_slot >= 0 ? (
  sb = BC_MY_REGION + bc_my_slot * BC_SLOT_SIZE;
  gmem[sb + 0] += 1;
  gmem[sb + 3] = bc_following;
  pbase = sb + 8;
  gmem[pbase + 0] = slider1;
  gmem[pbase + 1] = slider2;
  gmem[pbase + 2] = slider3;
  gmem[pbase + 3] = slider4;
  gmem[pbase + 4] = slider5;
  gmem[pbase + 5] = slider6;
  gmem[pbase + 6] = slider7;
  gmem[pbase + 7] = slider8;
  gmem[pbase + 8] = slider9;
  gmem[pbase + 9] = slider10;
  gmem[pbase + 10] = slider11;
  gmem[pbase + 11] = slider12;
  gmem[pbase + 12] = slider13;
  gmem[pbase + 13] = slider14;
);

bc_instance_count = 0;
bc_scan = 0;
loop(BC_MAX_INST,
  tsb = BC_MY_REGION + bc_scan * BC_SLOT_SIZE;
  tsid = gmem[tsb + 1];
  tsid > 0 ? (
    bc_scan == bc_my_slot ? (
      bc_instance_count += 1;
    ) : (
      tshb = gmem[tsb + 0];
      tshb == BC_STALE_HB[bc_scan] ? (
        BC_STALE_CT[bc_scan] += samplesblock / srate;
        BC_STALE_CT[bc_scan] > BC_STALE_TIMEOUT ? (
          gmem[tsb + 1] = 0;
          BC_STALE_CT[bc_scan] = 0;
        );
      ) : (
        BC_STALE_HB[bc_scan] = tshb;
        BC_STALE_CT[bc_scan] = 0;
        bc_instance_count += 1;
      );
    );
  );
  bc_scan += 1;
);

@sample
mono = (spl0 + spl1) * 0.5 * in_level;
// Input clamp — catches record-arm spikes before the amp sees them
mono = max(-1.0, min(1.0, mono));

// 1. Input stage (EF86 pentode grid physics)
input_intensity > 0.0 ? (
  mono = inp.input_stage_proc(mono, input_intensity);
);

// 2. Input coupling cap
mono = input_hp.bq_proc(mono);

// 3. PSU sag (shared rail)
bplus = psu.psu_proc(abs(mono) * (1.0 + gain * 2.0), sag_depth);

// 3b. v1.1: EF86 screen grid compression — pentode positive peak soft knee
// Screen grid current increases as signal approaches plate saturation
// Creates the "glassy" ceiling that's different from triode grid conduction
// Applied BEFORE ef86.triode_proc — shapes what the EF86 sees
ef86_drive = gain * 0.65;
ef86_in = mono;
screen_compress = 0;
ef86_in > 0.6 * bplus ? (
  screen_compress = ef86_in - (0.6 * bplus + tanh_approx((ef86_in - 0.6 * bplus) * 3.0) * 0.15 * bplus);
  ef86_in = 0.6 * bplus + tanh_approx((ef86_in - 0.6 * bplus) * 3.0) * 0.15 * bplus;
);

// 4. EF86 pentode preamp — the defining sound
ef86_out = ef86.triode_proc(ef86_in, ef86_drive, bplus,
  0.9,   // pos_hard: soft positive (screen current limiting)
  2.0,   // neg_hard: HARD negative (pentode cutoff — the AC15 signature)
  0.90,  // pos_ceil
  0.65,  // neg_ceil: LOW — pentode cuts off early on negative
  0.40,  // gc_thresh
  0.10   // gc_amount
);

// 4b. v1.1: Interstage coupling cap — 0.01uF × 100k = 160Hz HP
// Tighter bass than Marshall (0.022uF). Applied to both channels.
ef86_out = coupling_hp.bq_proc(ef86_out);

// 5. Top Boost channel: 12AX7 second stage
channel == 1 ? (
  v1_drive = gain * 0.45;
  amp_out = v1.triode_proc(ef86_out, v1_drive, bplus, 1.3, 1.1, 0.80, 0.95, 0.30, 0.15);
  amp_out = tb.topboost_proc(amp_out);
) : (
  amp_out = ef86_out;
);

amp_out *= 6.0;

// 5b. v1.1: Cathodyne phase inverter — THE Vox bloom
// At low master: transparent. At high master: asymmetric clipping = harmonic bloom
// Cathodyne: plate clips harder (1.0) than cathode (0.85) = inherent asymmetry
pi_drive = master * 0.40;
amp_out = pi.triode_proc(amp_out, pi_drive, bplus, 1.0, 0.85, 0.85, 0.90, 0.45, 0.08);

// 6. Effects loop (pre-power-amp — signal returns through EL84s + cab)
fx_bypass == 0 ? (
  // Tremolo (bias wobble)
  trem_depth > 0 ? (
    amp_out = trem.trem_proc(amp_out, trem_depth, trem_speed);
  );
  // Spring reverb
  spring_mix > 0 ? (
    amp_out = spring.spring_proc(amp_out, spring_mix);
  );
  // Safety clamp — catches runaway feedback before it hits power tubes
  amp_out = max(-0.95, min(0.95, amp_out));
);

// 7. Power amp (EL84 cathode-biased push-pull, NO NFB)
//    v1.1: Now includes output transformer saturation + winding resonance
amp_out *= master;
amp_out = pa.poweramp_proc(amp_out, bplus);

// 8. Soft gate
amp_out = gate.sgate_proc(amp_out);

// 9. Cabinet (Celestion Alnico Blue 1x12 + open-back resonance)
amp_out = cab1.bq_proc(amp_out);
amp_out = cab2.bq_proc(amp_out);
amp_out = cab3.bq_proc(amp_out);
amp_out = cab4.bq_proc(amp_out);
amp_out = cab5.bq_proc(amp_out);
amp_out = cab6.bq_proc(amp_out);
amp_out = cab_res.bq_proc(amp_out);

amp_out *= out_level;
// Brickwall output limiter — transparent safety ceiling
amp_out = max(-0.95, min(0.95, amp_out));
spl0 = amp_out;
spl1 = amp_out;

