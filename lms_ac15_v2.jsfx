cab3.bq_init(); cab3.bq_set_peak(800, -2.0, 1.2);
cab4.bq_init(); cab4.bq_set_peak(2500, 4.0, 1.8);
cab5.bq_init(); cab5.bq_set_peak(4500, -3.0, 1.0);
cab6.bq_init(); cab6.bq_set_lp(5500, 0.55);
// v1.1: Open-back 1x12 baffle resonance — the "body" of the Vox sound
cab_res.bq_init();
cab_res.bq_set_peak(180, 1.5, 2.0);

gain = 0.5; in_level = 1; out_level = 1; master = 0.5;
channel = 0; input_intensity = 0.5; sag_depth = 0.5;
tonecut = 0.5;
fx_bypass = 0;

  // ============================================================
  // BROADCAST SYSTEM — Instance Registry
  // ============================================================
  BC_BASE = 100000;
  BC_MY_TYPE = 14;
  BC_SLOT_SIZE = 512;
  BC_MAX_INST = 32;
  BC_PARAM_COUNT = 15;
  BC_MY_REGION = BC_BASE + BC_MY_TYPE * 16384;
